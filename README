Just some of my configuration files for dev environment. :)

# shell stuff
profile         -> .profile
tmux.conf       -> .tmux.conf
config.fish     -> .config/fish/config.fish
fish-functions/ -> .config/fish/functions/
Xdefaults       -> .Xdefaults

# i3 window manager n stuff
i3status.conf  -> .i3/status.conf
i3config.base  -> .i3/config.base
i3config.indal -> .i3/config.local (for example)
conkyrc        -> .conkyrc
conkyrc.laptop -> .conkyrc (for example)

sudo pip install i3-py
git clone 'git@github.com:OliverUv/quickswitch-for-i3.git' /home/oliver/Projects/quickswitch-for-i3

# video things
mpv-input.conf      -> .mpv/input.conf
youtube-viewer.conf -> .config/youtube-viewer/youtube-viewer.conf

# urxvt-256color
# requires xclip and xsel
urxvt-perls/keyboard-select -> .urxvt/ext/
urxvt-perls/url-select      -> .urxvt/ext/
urxvt-perls/clipboard       -> .urxvt/ext/
urxvt-font-size/font-size   -> .urxvt/ext/

  or all those to /usr/lib/urxvt/perl/

# programming
vimrc           -> .vimrc
vimrc.voyager   -> .vimrc.local (for example)
gvimrc          -> .gvimrc
dotvim          -> .vim
gitconfig       -> .gitconfig
gitignoreglobal -> .gitignoreglobal

# linters
coffeelint.json -> .config/coffeelint.json
flake8          -> .config/flake8

# firefox developer edition
# sudo add-apt-repository ppa:ubuntu-mozilla-daily/firefox-aurora
# sudo apt-get update
# sudo apt-get install firefox
vimperatorrc           -> .vimperatorrc
vimperatorplugin       -> .vimperator/plugin
vimperatorcolors       -> .vimperator/colors
vimperatorquickmarks   -> .vimperator/info/dev-edition-default/quickmarks
vimperatorignored-keys -> .vimperator/info/dev-edition-default/ignored-keys

# not in use
xprofile -> .xprofile
