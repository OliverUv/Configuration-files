# vim: ft=i3
# Host specific settings

exec --no-startup-id setxkbmap "en_US,se" -option 'ctrl:nocaps,grp:lalt_lshift_toggle'
exec --no-startup-id synergyc --daemon --name laptop 192.168.1.137
exec --no-startup-id xrandr --output HDMI1 --off --output LVDS1 --primary --mode 1600x900 --pos 11 --off --output VGA1 --mode 1280x1024 --pos 0x0 --rotate normal
